{
    "version": 2,
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
          "src": "/about",
          "dest": "./templates/about.html"
        },
        {
          "src": "/projects",
          "dest": "./templates/projects.html"
        },
        {
          "src": "/contact",
          "dest": "./templates/contact.html"
        },
        {
          "src": "/404",
          "dest": "./templates/404.html"
        },
        {
            "src": "/",
            "dest": "./templates/index.html"
        }
    ]
}